.container {
  padding: 20rpx 40rpx 40rpx 40rpx;
}
.warning-box {
  width: 100%;
  background-color: #F8EFA8;
  box-sizing: border-box;
  position: fixed;
  display: flex;
  flex-direction: row;
  padding: 20rpx 40rpx;
  align-items: center;
}
.wraning-text-blue {
  color:#2296F3;
  font-size: 24rpx;
  font-weight: normal;
}
.icon-close {
  font-size: 30rpx;
  position: absolute;
  right: 10rpx;
  color: #F96A0E;
  padding: 20rpx 40rpx;
  box-sizing: border-box;
}
.wraning-kong-box {
  height: 80rpx;
}
.card-item-img {
  width: 140rpx;
  height: 140rpx;
}
.medicines-title {
  font-size: 36rpx;
  font-weight: 500;
}
.card-item-text {
  font-size: 24rpx;
  color: #484848;
  width: 206rpx;
  height: 68rpx;
  text-align: center;
  margin-top: 10rpx;
}
.medicines-card-item {
  height: 220rpx;
  width: 214rpx;
  box-shadow: 0 2px 16px 0 rgba(0,0,0,0.10);
  border-radius: 10px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-right: 14rpx;
  margin-bottom: 26rpx;
  padding: 10rpx;
  box-sizing: border-box;
}
.medicines-card {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.medicines-card-item:nth-child(3n) {
  margin-right: 0rpx;
}
.medicines-main {
  margin-bottom: 50rpx;
}
.icon-what {
  color: #C9C9C9;
  font-size: 34rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.medicines-title-box {
  display: flex;
  flex-direction: row;
  align-items: center;
  text-align: center;
  padding: 10rpx 0rpx 30rpx 0rpx;
}
