  display: ï¬‚ex;
  flex-direction: column;
  align-items: center;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.usermotto {
  margin-top: 200px;
}
.usual-medicine-content {
  font-size: 30rpx;
  line-height: 40rpx;
  color: #999;
}
.modal-body {
  font-size: 32rpx;
  line-height: 44rpx;
}
.medicine-content {
  color:#2296F3;
}
